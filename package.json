{
  "name": "vue-cron-custom",
  "version": "1.0.9",
  "description": "vue 的 cron 组件，支持解析/反解析 cron 表达式，生成最近五次的符合条件时间",
  "author": "ibule",
  "license": "MIT",
  "main": "lib/vueCronCustom.umd.min.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:ibule/vue-cron-custom.git"
  },
  "homepage": "https://ibule.github.io/vCrontab/dist/",
  "keywords": [
    "cron",
    "vue",
    "vuecron",
    "crontab",
    "vue-crontab",
    "vueCronCustom"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lib": "vue-cli-service build --target lib --name vueCronCustom --dest lib src/index.js",
    "pub": "npm run-script build && npm publish"
  },
  "dependencies": {
    "element-ui": "^2.4.11",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "vue-template-compiler": "^2.5.17"
  }
}
